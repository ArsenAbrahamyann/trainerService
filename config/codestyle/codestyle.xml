<code_scheme name="GoogleCheckstyle">
    <option name="USE_SAME_INDENTS" value="true" />
    <JavaCodeStyleSettings>
        <option name="USE_FQ_CLASS_NAMES" value="false" />
        <option name="IMPORT_LAYOUT_TABLE">
            <value>
                <entry value="import java.*" />
                <entry value="import javax.*" />
                <entry value="import org.*" />
                <entry value="import com.*" />
                <entry value="import static all other imports" />
                <entry value="import all other imports" />
            </value>
        </option>
        <option name="FIELD_NAME_PREFIX" value="" />
        <option name="STATIC_FIELD_NAME_PREFIX" value="" />
        <option name="FIELD_NAME_SUFFIX" value="" />
        <option name="STATIC_FIELD_NAME_SUFFIX" value="" />
        <option name="PREFER_LONGER_NAMES" value="true" />
        <option name="ALIGN_GROUP_FIELD_DECLARATIONS" value="false" />
        <option name="ALIGN_MULTILINE_CHAINED_METHODS" value="false" />
        <option name="ALIGN_MULTILINE_PARAMETERS_IN_CALLS" value="true" />
        <option name="ALIGN_MULTILINE_BINARY_OPERATION" value="true" />
        <option name="ALIGN_MULTILINE_EXTENDS_LIST" value="true" />
        <option name="ALIGN_MULTILINE_PARAMETERS" value="true" />
        <option name="CLASS_NAMES" value="^[A-Z][A-Za-z0-9]*$" />
        <option name="METHOD_NAMES" value="^[a-z][A-Za-z0-9]*$" />
        <option name="FIELD_NAMES" value="^[a-z][A-Za-z0-9]*$" />
        <option name="PARAMETER_NAMES" value="^[a-z][A-Za-z0-9]*$" />
        <option name="LOCAL_VARIABLE_NAMES" value="^[a-z][A-Za-z0-9]*$" />
        <option name="CATCH_PARAMETER_NAMES" value="^[a-z][A-Za-z0-9]*$" />
    </JavaCodeStyleSettings>
    <codeStyleSettings language="JAVA">
        <option name="ALIGN_MULTILINE_METHOD_BRACKETS" value="false" />
        <option name="ALIGN_MULTILINE_PARAMETERS" value="true" />
        <option name="BINARY_OPERATION_WRAP" value="2" />
        <option name="BRACE_STYLE" value="1" />
        <option name="CLASS_BRACE_STYLE" value="1" />
        <option name="FIELD_ANNOTATION_WRAP" value="2" />
        <option name="METHOD_ANNOTATION_WRAP" value="2" />
        <option name="ALIGN_GROUP_FIELD_DECLARATIONS" value="false" />
        <option name="INDENT_CLASS_BODY" value="true" />
        <option name="INDENT_CASE_FROM_SWITCH" value="true" />
        <option name="MAX_LINE_LENGTH" value="120" />
        <option name="RIGHT_MARGIN" value="120" />
        <option name="SPACE_BEFORE_METHOD_PARENTHESES" value="false" />
        <option name="SPACE_BEFORE_COMMA" value="false" />
        <option name="SPACE_AFTER_COMMA" value="true" />
        <option name="KEEP_BLANK_LINES_IN_DECLARATIONS" value="1" />
        <option name="KEEP_BLANK_LINES_IN_CODE" value="1" />
        <option name="SPACE_BEFORE_FOR_PARENTHESES" value="false" />
        <option name="SPACE_AROUND_ASSIGNMENT_OPERATORS" value="true" />
        <option name="SPACE_AROUND_LOGICAL_OPERATORS" value="true" />
        <option name="SPACE_BEFORE_COLON_IN_CASE" value="false" />
        <option name="LINE_COMMENT_AT_FIRST_COLUMN" value="true" />
        <option name="BLOCK_COMMENT_AT_FIRST_COLUMN" value="true" />
    </codeStyleSettings>
</code_scheme>